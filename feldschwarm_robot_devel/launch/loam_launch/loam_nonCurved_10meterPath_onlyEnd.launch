<?xml version="1.0" encoding="UTF-8"?>

<launch>
  <include file="$(find feldschwarm_robot_devel)/launch/robot_description.launch"/>
  <include file="$(find feldschwarm_robot_devel)/launch/loam.launch" />
  <arg name="world" default="empty"/>
  <arg name="paused" default="false"/>
  <arg name="use_sim_time" default="true"/>
  <arg name="gui" default="true"/>
  <arg name="headless" default="false"/>
  <arg name="debug" default="false"/>

  


  

  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    <arg name="world_name" value="$(find feldschwarm_robot_devel)/worlds/nonCurvedScenario/10_meterPath/2_lm_onlyEndPoint.world"/>
    <arg name="paused" value="$(arg paused)"/>
    <arg name="use_sim_time" value="true"/>
    <arg name="gui" value="$(arg gui)"/>
    <arg name="headless" value="$(arg headless)"/>
    <arg name="debug" value="$(arg debug)"/>
  </include>
  
  <node pkg="tf2_ros" type="static_transform_publisher" name="transform_broadcaster" args="0 0 0.638  0 0 0   odom camera_init"/>
  <node pkg="feldschwarm_robot_devel" type="odom_listener" name="odom_listener" output="screen"/>

  

  <!--launch rviz-->
  <node name="rviz" pkg="rviz" type="rviz" args="-d $(find feldschwarm_robot_devel)/launch/loam_scnr_1.rviz"/>
   <!-- args="-d $(find feldschwarm_robot_devel)/launch/loam_scnr_1.rviz"-->
</launch>
