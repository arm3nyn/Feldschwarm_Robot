<launch>
  <arg name="use_sim_time" default="true" />
  <arg name="gui" default="true" />
  <arg name="headless" default="false" />
  <arg name="world_name" default="worlds/empty.world"/>
  
<!-- Control 
<arg name="front_laser" default="true" />
  <arg name="default_config" value="front_laser" if="$(arg front_laser)" />
  <arg name="default_config" value="base" unless="$(arg front_laser)" />
-->
<include file="$(find gazebo_ros)/launch/empty_world.launch">
    <arg name="world_name" value="$(find master_arbeit_pkg)test_package/world/scenario1_v2.world" />
    <arg name="debug" value="0" />
    <arg name="gui" value="$(arg gui)" />
    <arg name="use_sim_time" value="$(arg use_sim_time)" />
    <arg name="headless" value="$(arg headless)" />
    
  </include>

  <!--param name="robot_description" command="$(find xacro)/xacro '$(find test_package)/urdf/feldschwarm.gazebo.xacro'" /-->
  <param name="robot_description" command="$(find xacro)/xacro '$(find master_arbeit_pkg)test_package/urdf/feldschwarm.gazebo.xacro'" />

                    
  <node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher" />

  <!-- Control 
  <include file="$(find robot_control)/launch/control.launch" />
  <include file="$(find robot_control)/launch/teleop.launch">
    <arg name="joystick" value="false"/>
  </include>
  -->
  <!-- Spawn Jackal 
  <node name="urdf_spawner" pkg="gazebo_ros" type="spawn_model"
              args="-urdf -model feldschwarm -param robot_description -x 0 -y 0 -z 1.0" />
 -->
  <!-- Rviz 
  <node pkg="rviz" type="rviz" name="rviz" args="-d $(find robot_velodyne)/launch/include/run.rviz" />
 -->
  <!-- TF 
  <node pkg="robot_velodyne" type="tf_publisher"    name="tf_publisher"    output="screen"/>
  
 -->
  <node name="spawn_feldschwarmRobot" pkg="gazebo_ros" type="spawn_model" args="-unpause -urdf -param robot_description -model feldschwarm_robot"/>




</launch>