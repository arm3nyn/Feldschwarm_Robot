<?xml version="1.0" ?>
<sdf version="1.5">
  <world name="default">

    <!-- A global light source -->
    <include>
      <uri>model://sun</uri>
    </include>

    <!-- A ground plane -->
    <include>
      <uri>model://my_ground_plane</uri>
    </include>

    <scene>
      <sky>
        <clouds>
          <speed>12</speed>
        </clouds>
      </sky>
    </scene>

    <model name="Testoval_Inseln">
      <include>
        <uri>model://Mittelinseln</uri>
      </include>
    </model>

    <model name="Testoval">
      <include>
        <uri>model://Testoval</uri>
      </include>
    </model>

    <!-- A testing model that includes the test robot model 
    <model name="Feldschwarm_Roboter_01">
      <include>
        <uri>model://Feldschwarm_Roboter</uri>
      </include>
-->
      <!-- Attach the plugin to this model 
      <plugin name="steering_control" filename="build/libsteering_plugin.so"/>
    </model>
     -->
    <physics type="ode">
      <max_step_size>0.001</max_step_size>
      <real_time_factor>1</real_time_factor>
      <real_time_update_rate>10000</real_time_update_rate>
      <ode>
        <solver>
          <type>quick</type>
          <iters>1000</iters>
          <sor>1.3</sor>
        </solver>
        <constraints>
          <cfm>0</cfm>
          <erp>0.2</erp>
          <contact_max_correcting_vel>100</contact_max_correcting_vel>
          <contact_surface_layer>0.001</contact_surface_layer>
        </constraints>
      </ode>
    </physics>

  </world>
</sdf>
